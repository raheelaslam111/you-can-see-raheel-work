<?xml version="1.0" encoding="utf-8" ?>
<odoo>
        <record id="nms_csv_attachment_download_view" model="ir.ui.view">
            <field name="name">nms.csv.attachment.download.form</field>
            <field name="model">nms.csv.attachment.download</field>
            <field name="arch" type="xml">
               <form>
                   <group>
                        <group string="Download you Attachment">
                                <field name="name" invisible="1"/>
                                <field name="downloadable_attachment" filename="name"/>
                        </group>
                        <footer>
                            <button string="Discard" class="btn-default" special="cancel"/>
                        </footer>
                    </group>
               </form>
            </field>
        </record>

